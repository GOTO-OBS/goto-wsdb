    petroMag_g real,
    petroMag_r real,
    petroMag_i real,
    petroMag_z real,
    petroMagErr_u real,
    petroMagErr_g real,
    petroMagErr_r real,
    petroMagErr_i real,
    petroMagErr_z real,
    petroR50_r real,
    petroR90_r real,
    modelMag_u real,
    modelMag_g real,
    modelMag_r real,
    modelMag_i real,
    modelMag_z real,
    modelMagErr_u real,
    modelMagErr_g real,
    modelMagErr_r real,
    modelMagErr_i real,
    modelMagErr_z real,
    cModelMag_u real,
    cModelMag_g real,
    cModelMag_r real,
    cModelMag_i real,
    cModelMag_z real,
    cModelMagErr_u real,
    cModelMagErr_g real,
    cModelMagErr_r real,
    cModelMagErr_i real,
    cModelMagErr_z real,
